{% macro stylesheetspush(form) %}
    <link href="{{ asset('bundles/appcore/select2/css/select2.min.css') }}" rel="stylesheet" />
    {{ form_stylesheet(form) }}
{% endmacro %}

{% macro javascriptspush(form) %}
    <script src="{{ asset('bundles/appcore/tinymce/tinymce.min.js') }}"></script>
    <script src="{{ asset('bundles/appcore/select2/js/select2.full.min.js') }}"></script>
    {{ form_javascript(form) }}
{% endmacro %}